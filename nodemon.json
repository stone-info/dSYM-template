{
  "restartable" : "rs",
  "ignore"      : [
    ".git",
    "node_modules/**/node_modules",
    "request_test/**/*",
    "client/**/*"
  ],
  "verbose"     : false,
  "execMap"     : {
    "js" : "node --harmony"
  },
  "watch"       : [
    "server/",
    "src/views",
    "views",
    "*"
  ],
  "ext"         : "js json"
}
